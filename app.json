{
  "expo": {
    "name": "FriendFinder",
    "slug": "friendfinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "friendfinder",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FriendFinder to use your location to find nearby friends."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow FriendFinder to access your photos to set a profile picture."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}